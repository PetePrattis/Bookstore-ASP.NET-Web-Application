@model IEnumerable<WorldHistoryBookStore.Models.publisher>

@{
    ViewBag.Title = "Index";
}

<style>
    .table {
        background-color: white;
    }
</style>

<h2>Publishers</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>

        <th>
            @*@Html.DisplayNameFor(model => model.pub_id)*@ ID
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pub_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.city)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.state)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.country)
        </th>
        <th>
            @* @Html.DisplayNameFor(model => model.pub_info.pr_info)*@ Info
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.pub_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.pub_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.city)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.state)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.country)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.pub_info.pr_info)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.pub_id }) |
            @Html.ActionLink("Details", "Details", new { id = item.pub_id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.pub_id })
        </td>
    </tr>
}

</table>
