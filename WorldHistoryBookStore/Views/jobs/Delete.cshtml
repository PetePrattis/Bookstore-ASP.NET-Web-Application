@model WorldHistoryBookStore.Models.job

@{
    ViewBag.Title = "Delete";
}
<style>
    .main {
        background-color: white;
        border-color: black;
        border-width: thin;
        border-style: solid;
        border-radius: 10px;
        padding: 10px;
    }
</style>
<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>

<h4>Job</h4>
<hr />
<div class="main">
    <dl class="dl-horizontal">
        <dt>
            @* @Html.DisplayNameFor(model => model.job_desc)*@ Job Description
        </dt>

        <dd>
            @Html.DisplayFor(model => model.job_desc)
        </dd>

        <dt>
            @*@Html.DisplayNameFor(model => model.min_lvl)*@ Minimum Level
        </dt>

        <dd>
            @Html.DisplayFor(model => model.min_lvl)
        </dd>

        <dt>
            @*@Html.DisplayNameFor(model => model.max_lvl)*@ Maximum Level
        </dt>

        <dd>
            @Html.DisplayFor(model => model.max_lvl)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-primary" />
        </div>
    }
</div>

@Html.ActionLink("Back to List", "Index")