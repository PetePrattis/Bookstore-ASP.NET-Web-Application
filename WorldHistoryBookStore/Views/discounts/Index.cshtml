@model IEnumerable<WorldHistoryBookStore.Models.discount>

@{
    ViewBag.Title = "Index";
}
<style>
    .table {
        background-color: white;
    }
</style>

<h2>Discounts</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
       
        <th>
            @*@Html.DisplayNameFor(model => model.discounttype)*@ Discount Type
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.store.stor_name)*@ Store Name
        </th>
        <th>
            @* @Html.DisplayNameFor(model => model.lowqty) *@ Low Quantity
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.highqty)*@ High Quantity
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.discount1)*@ Discount
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.discounttype)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.store.stor_name)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.lowqty)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.highqty)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.discount1)
        </td>
        <td>
            @* @Html.ActionLink("Edit", "Edit") |
        @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
        @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })*@
        </td>
    </tr>
}

</table>
